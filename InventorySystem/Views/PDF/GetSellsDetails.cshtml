@model IEnumerable<InventorySystem.DataBase.Sell_t>

@{
    ViewBag.Title = "Salida de Inventario por Producto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Salida de Inventario por Producto</h2>

<p>
    @Html.ActionLink("Print Sale", "PrintSaleDatail")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cash)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.discount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Operation_type_t.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Person_t.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_t.email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.total)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cash)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.discount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Operation_type_t.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Person_t.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_t.email)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>*@
    </tr>
}

</table>
